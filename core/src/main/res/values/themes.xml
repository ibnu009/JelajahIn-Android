<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="customRatingBar"
        parent="android:style/Widget.Material.RatingBar.Small">
        <item name="colorControlNormal">@color/dusk_yellow</item>
        <item name="colorControlActivated">@color/dusk_yellow</item>
        <item name="android:progressTint">@color/dusk_yellow</item>
    </style>

    <style name="JelajahinPrimaryButton">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_rounded_corner_solid</item>
    </style>

    <style name="JelajahinTextButton">
        <item name="android:textColor">@color/green</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="JelajahinPrimaryButtonBorder">
        <item name="android:textColor">@color/dark_green_2</item>
        <item name="android:background">@drawable/bg_rounded_corner_stroke</item>
    </style>

</resources>