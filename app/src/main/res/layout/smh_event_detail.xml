<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="255dp"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view4"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />


    <View
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton"
        app:layout_constraintStart_toEndOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <View
        android:id="@+id/view5"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:layout_marginEnd="@dimen/space_xxtra"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <View
        android:id="@+id/view7"
        android:layout_width="250dp"
        android:layout_height="65dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6" />

    <View
        android:id="@+id/view8"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <View
        android:id="@+id/view9"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space_double"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <View
        android:id="@+id/view10"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_marginHorizontal="@dimen/space_double"
        android:layout_marginTop="@dimen/space"
        android:background="@drawable/skeleton_darker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view10" />


</androidx.constraintlayout.widget.ConstraintLayout>